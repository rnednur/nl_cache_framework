# PostgreSQL Connection Details (used if DATABASE_URL is not set)
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=mcp_cache_db

# Database Schema Configuration
DB_SCHEMA=public

# Alternative: Provide the full database connection URL
# DATABASE_URL=postgresql://user:password@host:port/dbname

# Sentence Transformer Model (Optional - defaults in code)
# DEFAULT_MODEL_NAME=sentence-transformers/all-mpnet-base-v2

# Similarity Threshold (Optional - defaults in code)
# SIMILARITY_THRESHOLD=0.85

# Debug Mode for Server (Optional - defaults to False)
# DEBUG=true 

# OpenRouter LLM Integration
# OPENROUTER_API_KEY=your_api_key_here
# OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
# OPENROUTER_MODEL=google/gemini-flash-2.5 